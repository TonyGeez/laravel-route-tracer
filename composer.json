{
  "name": "tonygeez/laravel-route-tracer",
  "description": "Runtime execution tracer for Laravel routes - captures actual file dependencies and execution flow",
  "license": "MIT",
  "keywords": ["laravel",
    "tracing",
    "debugging",
    "profiling",
    "route",
    "dependencies"],
  "authors": [{
    "name": "TonyGeez",
    "email": "hixostorage@gmail.com"
  }],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/routing": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.23|^7.0|^8.0|^9.0|^10.0",
    "phpunit/phpunit": "^9.4|^10.1|^11.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "TonyGeez\\LaravelRouteTracer\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TonyGeez\\LaravelRouteTracer\\TraceRouteDependenciesServiceProvider"
      ],
      "aliases": {
        "RouteTracer": "TonyGeez\\LaravelRouteTracer\\Facades\\RouteTracer"
      }
    }
  }
}